{
    "name": "smashed-egg/laravel-console",
    "description": "Provides enhancements to commands in Laravel",
    "keywords": ["php", "laravel", "console"],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/smashed-egg/laravel-console/issues",
        "source": "https://github.com/smashed-egg/laravel-console"
    },
    "require": {
        "php": "^8.0.2"
    },
    "require-dev": {
        "orchestra/testbench": "^7.6",
        "laravel/framework": "^9.21"
    },
    "autoload": {
        "psr-4": {
            "SmashedEgg\\LaravelConsole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmashedEgg\\LaravelConsole\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tom Ellis"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    }
}
