{
    "name": "smashed-egg/laravel-console",
    "description": "This package allows you to enhance your Console Commands by making it easier to add aliases and use interactive functionality.",
    "keywords": ["php", "laravel", "console"],
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/smashed-egg/laravel-console/issues",
        "source": "https://github.com/smashed-egg/laravel-console"
    },
    "require": {
        "php": "^8.0.2",
        "illuminate/console": "^9.21|^10.0",
        "illuminate/contracts": "^9.21|^10.0",
        "illuminate/support": "^9.21|^10.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.6|^8.0",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "SmashedEgg\\LaravelConsole\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SmashedEgg\\LaravelConsole\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Tom Ellis"
        }
    ],
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
